{
  "name": "shopify-starter-theme",
  "version": "0.0.1",
  "description": "A Shopify starter theme using Theme Kit and Webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "watch:assets": "webpack --watch",
    "watch:theme": "theme watch --dir theme",
    "prod:build": "NODE_ENV=production webpack -p",
    "prod:deploy": "NODE_ENV=production webpack -p && theme deploy --dir theme"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "read-yaml": "^1.1.0",
    "style-loader": "^1.1.3",
    "tailwindcss": "^1.2.0",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "lazysizes": "^5.2.0",
    "vue": "^2.6.11"
  }
}
