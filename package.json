{
  "name": "shopify-starter-theme",
  "version": "0.0.1",
  "description": "A Shopify starter theme using Theme Kit and Webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "deploy": "NODE_ENV=production webpack -p && theme deploy --dir=theme",
    "watch:assets": "webpack --watch",
    "watch:theme": "theme watch --notify=theme-ready.tmp --dir=theme",
    "prod:build": "NODE_ENV=production webpack -p",
    "prod:deploy": "NODE_ENV=production webpack -p && theme deploy --env=production --dir=theme"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "read-yaml": "^1.1.0",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@shopify/theme-a11y": "^3.0.3",
    "@shopify/theme-cart": "^3.0.3",
    "@shopify/theme-currency": "^3.0.3",
    "@shopify/theme-images": "^3.0.3",
    "@shopify/theme-product": "^3.1.0",
    "@shopify/theme-product-form": "^3.1.0",
    "autoprefixer": "^9.7.6",
    "core-js": "^3.6.4",
    "cssnano": "^4.1.10",
    "lazysizes": "^5.2.2",
    "normalize.css": "^8.0.1",
    "regenerator-runtime": "^0.13.5",
    "tailwindcss": "^1.2.0"
  }
}
