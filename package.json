{
  "name": "shopify-starter-theme",
  "version": "0.0.1",
  "description": "A Shopify starter theme using Theme Kit and Webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "deploy": "NODE_ENV=production webpack -p && theme deploy --dir=theme",
    "watch:assets": "webpack --watch",
    "watch:theme": "theme watch --notify=theme-ready.tmp --dir=theme",
    "prod:build": "NODE_ENV=production webpack -p",
    "prod:deploy": "NODE_ENV=production webpack -p && theme deploy --env=production --dir=theme"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "read-yaml": "^1.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "svelte": "^3.24.1",
    "svelte-loader": "^2.13.6",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@shopify/theme-a11y": "^3.1.0",
    "@shopify/theme-addresses": "^4.0.0",
    "@shopify/theme-cart": "^3.1.0",
    "@shopify/theme-currency": "^3.1.0",
    "@shopify/theme-images": "^3.1.0",
    "@shopify/theme-product": "^3.1.0",
    "@shopify/theme-product-form": "^3.1.0",
    "autoprefixer": "^9.8.6",
    "core-js": "^3.6.5",
    "cssnano": "^4.1.10",
    "lazysizes": "^5.2.2",
    "normalize.css": "^8.0.1",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss": "^1.8.6"
  }
}
